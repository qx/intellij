<templateSet group="Kotlin">
  <template name="test" value=" @Test&#10; @Throws(Exception::class)&#10; fun test_$method$_$describe$() {&#10; &#10; }   " description="test suit" toReformat="false" toShortenFQNames="true">
    <variable name="method" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="describe" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="KOTLIN" value="true" />
    </context>
  </template>
  <template name="***" value="myPrintln(&quot;*******************$VAR$******************&quot;)" description="println" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="KOTLIN" value="true" />
    </context>
  </template>
  <template name="cmt" value="//*******************************************$var$  Start*******************************************&#10;&#10;&#10;//*******************************************$var$  End  *******************************************" description="comment start end" toReformat="true" toShortenFQNames="true">
    <variable name="var" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="KOTLIN" value="true" />
    </context>
  </template>
  <template name="by" value=" :$Type$ by Delegates.observable($init$) { prop, old, new -&gt;&#10;        println(&quot;${getCurrentTime()} ------$name$ changed---: ${old} -&gt; ${new} &quot;)&#10;        }" description="observable" toReformat="false" toShortenFQNames="true">
    <variable name="Type" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="init" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="KOTLIN" value="true" />
    </context>
  </template>
</templateSet>